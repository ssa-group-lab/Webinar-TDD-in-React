{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react",
    "plugin:jest/recommended",
    "plugin:testing-library/recommended",
    "plugin:testing-library/react"
  ],
  "rules": {
    "reacct/prop-types": 0,
    "no-console": 1,
    "jest/no-interpolation-in-snapshots": 0,
    "jest/expect-expect": 0
  },
  "plugins": ["react", "import", "jsx-a11y", "jest", "testing-library"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
